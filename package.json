{
  "name": "configcreator",
  "version": "1.0.0",
  "description": "A html form which takes an json as input and gives an json as output.",
  "main": "index.php",
  "scripts": {
    "test": "./vendor/bin/phpunit tests",
    "commit": "cz",
    "postinstall": "husky install"
  },
  "keywords": [
    "generator",
    "html",
    "config"
  ],
  "devDependencies": {
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^12.1.1",
    "commitizen": "^4.2.3",
    "cz-conventional-changelog": "^3.2.0",
    "husky": "^6.0.0"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "author": "Peter Dohmen",
  "license": "ISC"
}
